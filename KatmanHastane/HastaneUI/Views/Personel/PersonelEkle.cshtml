@model EntityLayer.Personel;
@{
    ViewData["Title"] = "PersonelEkle";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<h2>Personel Ekle</h2>
<form asp-action="PersonelEkle" asp-controller="Personel" method="post">
    <div class="mb-3">
        <label class="form-label">PERSONEL ADI</label>
        <input asp-for="personel_Adi" type="text" class="form-control">

    </div>
    <div class="mb-3">
        <label class="form-label">PERSONEL SOYADI</label>
        <input type="text" class="form-control" asp-for="personel_Soyadi">
    </div>
    <button type="submit" class="btn btn-primary">PERSONEL EKLE</button>
</form>

